hackerrank prgm 1

#include<stdio.h>
int main()
{
int n;
scanf("%d",&n);
if(n>0 && (n&(n-1))==0)
printf("yes");
else
printf("no");
return 0;
}

hackerrank prgm 2
#include <stdio.h>

int main()
{
    int start,end;
    scanf("%d %d", &start,&end);
    int i= start;
    while(i <= end)
    {
        if(i%2==0){
            printf("%d",i);
        }
    i++;//

    return 0;
}
 }

hackerrank prgm 3

#include <stdio.h>

int main()
{
    int end;
    scanf("%d", &end);
    int i= 1;
    while(i <= end)
    {
            printf("%d",i);
            i++;
}
    return 0;
 }
hackerrank prgm4

#include <stdio.h>

int main()
{
    int end,i;
    scanf("%d", &end);
    for(i=1;i<=end;i++)
    {
        if(i%2!=0)
{
            printf("%d",i);
            
}
    }
}

hackerrank prgm 5

#include <stdio.h>

int main() {
    int multiplicand, n;
    scanf("%d %d", &multiplicand, &n);


    for (int i = 1; i <= multiplicand; i++) {
        printf("%d * %d = %d\n", i, n, i * n);
    }

    return 0;
}

hackerrank prgm 6

#include <stdio.h>

int main()
{
int a,b,c=0;
scanf("%d %d",&a,&b);
while(a>=b)
{
 a=a-b;
 c++;
}
printf("%d",c);

}

